<?xml version="1.0" encoding="UTF-8"?>
<actions>
  <action>
    <actionName>build</actionName>
    <packagings>
      <packaging>*</packaging>
    </packagings>
    <goals>
      <goal>generate-sources</goal>
      <goal>install</goal>
      <goal>-Dmaven.test.skip=true</goal>
    </goals>
  </action>
  <action>
    <actionName>rebuild</actionName>
    <packagings>
      <packaging>*</packaging>
    </packagings>
    <goals>
      <goal>clean</goal>
      <goal>generate-sources</goal>
      <goal>install</goal>
      <goal>-Dmaven.test.skip=true</goal>
    </goals>
  </action>
</actions>
